{
  "name": "caseyagollan.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "git submodule update --remote --merge && node -e \"console.log('ENV CHECK:', {NODE_ENV: process.env.NODE_ENV, WEBMENTION_IO_TOKEN: process.env.WEBMENTION_IO_TOKEN ? 'present' : 'missing'})\" && eleventy",
    "build-ghpages": "eleventy",
    "start": "eleventy --serve --port 8080",
    "dev": "npm run start",
    "serve": "serve . -p 8080",
    "update-library": "node utils/librarything-to-graph.js",
    "update-content": "git submodule update --remote --merge"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@chrisburnell/eleventy-cache-webmentions": "^2.1.11",
    "dotenv": "^17.2.2",
    "fs": "^0.0.1-security",
    "image-size": "^1.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "sanitize-html": "^2.17.0",
    "sass": "^1.93.2",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "serve": "^14.2.1",
    "wrangler": "^4.40.3"
  }
}
